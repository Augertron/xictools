#######################################################################
# mozy/src/gtkmozy/Makefile.in
#######################################################################
# $Id: Makefile.in,v 1.28 2014/02/15 18:40:33 stevew Exp $
#######################################################################

DATE = @DATE@

CXX = @CXX@
CC = @CC@
CFLAGS = @CFLAGSG@ @TOOLKITCFLAGS@ @UFLAGS@
DEPEND_PROG = @DEPEND_PROG@ @CFLAGSG@ @TOOLKITCFLAGS@
FILTER = @FILTER@

LPATH = ../../../xt_base
INCLUDE = -I../../include -I$(LPATH)/ginterf \
 -I$(LPATH)/ginterf/gtkinterf -I$(LPATH)/miscutil \
 @TOOLKITINC@

HFILES = \
    gtkhelp.h \
    gtkviewer.h

CCFILES = \
    gtkhelp.cc \
    gtkhttpmon.cc \
    gtkviewer.cc

CCOBJS = $(CCFILES:.cc=.o)
LIBOBJS = $(CCOBJS)

stamp: $(LIBOBJS)
	touch stamp

.cc.o:
	$(CXX) -c $(CFLAGS) $(INCLUDE) -o $@ $<

files::
	@echo [ $(CCFILES) $(HFILES) Makefile.in ]

subdirs::
	@echo ]

clean:
	-@rm -f $(CCOBJS) stamp

distclean: clean
	-@rm -f Makefile

depend:
	@echo depending in mozy/src/gtkmozy
	@if [ x$(DEPEND_DONE) = x ]; then \
	    echo DEPEND_DONE = 1 >> Makefile; \
	    $(DEPEND_PROG) $(INCLUDE) $(CCFILES) $(FILTER) >> Makefile; \
	fi

install:

uninstall:

#######################################################################
