#######################################################################
# xic/wrspif/Makefile.in
#######################################################################
# $Id: Makefile.in,v 1.1 2012/05/16 17:56:38 stevew Exp $
#######################################################################

GFX_VERSION = @GFX_VERSION@
DATE = @DATE@

CXX = @CXX@
CC = @CC@
CFLAGS = @CFLAGSG@ @NEEDINT64@ @UFLAGS@
DEPEND_PROG = @DEPEND_PROG@ @CFLAGSG@ @NEEDINT64@
AR = @AR@
RANLIB = @RANLIB@
FILTER = @FILTER@

LIB_TARGET = wrspif.a

LPATH = ../../../xt_base
SPFILES = hash.cc paramsub.cc spnumber.cc spparse.cc
HFILES =
CCFILES = $(SPFILES)
CCOBJS = $(CCFILES:.cc=.o)
INCLUDE = -I../../include -I$(LPATH)/ginterf -I$(LPATH)/miscutil

$(LIB_TARGET): $(CCOBJS)
	@if [ -f $(LIB_TARGET) ]; then \
	    rm -f $(LIB_TARGET); \
	fi
	$(AR) cr $(LIB_TARGET) $(CCOBJS)
	$(RANLIB) $(LIB_TARGET)

.cc.o:
	$(CXX) $(CFLAGS) $(INCLUDE) -c $*.cc

$(SPFILES):
	cp -f $(LPATH)/spnumber/$@ .

hash.cc: $(LPATH)/spnumber/hash.cc
paramsub.cc: $(LPATH)/spnumber/paramsub.cc
spnumber.cc: $(LPATH)/spnumber/spnumber.cc
spparse.cc: $(LPATH)/spnumber/spparse.cc

depend: $(SPFILES)
	@echo depending in xic/wrspif
	@if [ x$(DEPEND_DONE) = x ]; then \
	    echo DEPEND_DONE = 1 >> Makefile; \
	    $(DEPEND_PROG) $(INCLUDE) $(CCFILES) $(FILTER) >> Makefile; \
	fi

clean:
	-rm *.o $(LIB_TARGET)

distclean:
	-rm *.o $(LIB_TARGET) Makefile $(SPFILES)

#######################################################################
