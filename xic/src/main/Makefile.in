#######################################################################
# xic/main/Makefile.in
#######################################################################
# $Id: Makefile.in,v 5.91 2015/04/13 04:13:55 stevew Exp $
#######################################################################

GFX_VERSION = @GFX_VERSION@
DATE = @DATE@

CXX = @CXX@
CC = @CC@
CFLAGS = @CFLAGSG@ @NEEDINT64@ @UFLAGS@
DEPEND_PROG = @DEPEND_PROG@ @CFLAGSG@ @NEEDINT64@
AR = @AR@
RANLIB = @RANLIB@
FILTER = @FILTER@

LIB_TARGET = main.a

LPATH = ../../../xt_base
HFILES =
CCFILES = \
  attr_menu.cc cell_menu.cc cfilter.cc cvrt.cc cvrt_menu.cc \
  cvrt_setif.cc cvrt_variables.cc drcif.cc editif.cc errorlog.cc \
  errorlog_setif.cc events.cc expand.cc extif.cc file_menu.cc \
  funcs_cvrt.cc funcs_misc1.cc funcs_misc2.cc funcs_misc3.cc ghost.cc \
  grcalls.cc hardcopy.cc help_menu.cc info.cc init.cc keymacro.cc \
  keymap.cc kwstr_cvt.cc kwstr_ext.cc kwstr_lyr.cc kwstr_phy.cc \
  layers.cc layertab.cc layertab_setif.cc line45.cc main.cc \
  main_scriptif.cc main_setif.cc main_techif.cc main_txtcmds.cc \
  main_variables.cc measure.cc memory.cc menu.cc misc_menu.cc \
  modeswitch.cc oa_if.cc onexit.cc open.cc pcell.cc promptline.cc \
  promptline_setif.cc prpty.cc pushpop.cc python_if.cc save.cc \
  scedif.cc select.cc signals.cc subwin.cc tcltk_if.cc user_menu.cc \
  view_menu.cc
CCOBJS = $(CCFILES:.cc=.o)
INCLUDE = -I../../include -I$(LPATH)/ginterf -I$(LPATH)/miscutil \
    -I../../../secure -I$(LPATH)/malloc

$(LIB_TARGET): $(CCOBJS)
	@if [ -f $(LIB_TARGET) ]; then \
	    rm -f $(LIB_TARGET); \
	fi
	$(AR) cr $(LIB_TARGET) $(CCOBJS)
	$(RANLIB) $(LIB_TARGET)

.cc.o:
	$(CXX) $(CFLAGS) $(INCLUDE) -c $*.cc

depend:
	@echo depending in xic/main
	@if [ x$(DEPEND_DONE) = x ]; then \
	    echo DEPEND_DONE = 1 >> Makefile; \
	    $(DEPEND_PROG) $(INCLUDE) $(CCFILES) \
  $(FILTER) >> Makefile; \
	fi

clean:
	-rm *.o $(LIB_TARGET)

distclean:
	-rm *.o $(LIB_TARGET) Makefile

#######################################################################
