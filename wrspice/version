#! /bin/sh
#
# This is where the release numbers are set.
#

release="4"
tools_version="4.2"
spice_version="4.2.17"
devlib_version="4.2.17"

if [ z$1 != "z" ]; then
    case $1 in
    release)
        echo $release
        ;;
    tools)
        echo $tools_version
        ;;
    wrspice)
        echo $spice_version
        ;;
    devlib)
        echo $devlib_version
        ;;
    esac
fi

