#######################################################################
# wrspice/inp/Makefile.in
#######################################################################
# $Id: Makefile.in,v 2.58 2016/03/13 03:23:17 stevew Exp $
#######################################################################

SPICE_VERSION = @SPICE_VERSION@
DATE = @DATE@

CXX = @CXX@
CC = @CC@
CFLAGS = @CFLAGS_SG@ @UFLAGS@ -DWRSPICE
DEPEND_PROG = @DEPEND_PROG@ @CFLAGS_SG@ -DWRSPICE
AR = @AR@
RANLIB = @RANLIB@
FILTER = @FILTER@

LIB_TARGET = inp.a

LPATH = ../../../xt_base
INCLUDE = -I../../include -I$(LPATH)/ginterf -I$(LPATH)/miscutil

HFILES =
CCFILES = \
  inpdeck.cc inpdev.cc inpdotcd.cc inperror.cc inpfuncs.cc inpmodel.cc \
  inpptree.cc inptabpa.cc inptoken.cc inptran.cc
CCOBJS = $(CCFILES:.cc=.o)

$(LIB_TARGET): $(CCOBJS)
	@if [ -f $(LIB_TARGET) ]; then \
	    rm -f $(LIB_TARGET); \
	fi
	$(AR) cr $(LIB_TARGET) $(CCOBJS)
	$(RANLIB) $(LIB_TARGET)

.cc.o:
	$(CXX) $(CFLAGS) $(INCLUDE) -c $*.cc

depend:
	@echo depending in wrspice/inp
	@if [ x$(DEPEND_DONE) = x ]; then \
	    echo DEPEND_DONE = 1 >> Makefile; \
	    $(DEPEND_PROG) $(INCLUDE) $(CCFILES) $(FILTER) >> Makefile; \
	fi

clean:
	-rm *.o $(LIB_TARGET)

distclean:
	-rm *.o $(LIB_TARGET) Makefile

#######################################################################
