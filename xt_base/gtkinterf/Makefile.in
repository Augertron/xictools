#######################################################################
LOCATION = xt_base/gtkinterf
#######################################################################

DATE = @DATE@

CXX = @CXX@
CC = @CC@
CFLAGS = @CFLAGSG@ @TOOLKITCFLAGS@ @UFLAGS@
DEPEND_PROG = @DEPEND_PROG@ @CFLAGSG@ @TOOLKITCFLAGS@
AR = @AR@
RANLIB = @RANLIB@
FILTER = @FILTER@

INCLUDE = -I../include -I../include/gtkinterf -I../include/ginterf \
  -I../include/miscutil @TOOLKITINC@

LIB_TARGET = ../lib/gtkinterf.a

HFILES = \
    gtkedit.h \
    gtkfile.h \
    gtkfont.h \
    gtkhcopy.h \
    gtkinterf.h \
    gtklist.h \
    gtkmcol.h \
    gtkpfiles.h \
    gtksearch.h \
    gtkspinbtn.h \
    gtkutil.h \
    gtkx11.h \
    mswdraw.h \
    mswpdev.h

CCFILES = \
    gtkedit.cc \
    gtkfile.cc \
    gtkfont.cc \
    gtkhcopy.cc \
    gtkinterf.cc \
    gtklist.cc \
    gtkmcol.cc \
    gtkpfiles.cc \
    gtksearch.cc \
    gtkspinbtn.cc \
    gtkutil.cc \
    gtkxdraw.cc \
    mswdraw.cc \
    mswpdev.cc

CCOBJS = $(CCFILES:.cc=.o)

$(LIB_TARGET): $(CCOBJS)
	@if [ -f $(LIB_TARGET) ]; then \
	    rm -f $(LIB_TARGET); \
	fi
	$(AR) cr $(LIB_TARGET) $(CCOBJS)
	$(RANLIB) $(LIB_TARGET)

.cc.o:
	$(CXX) -c $(CFLAGS) $(INCLUDE) -o $@ $<

files::
	@echo [ $(CCFILES) $(HFILES) Makefile.in ]

subdirs::
	@echo ]

clean:
	-@rm -f $(CCOBJS) $(LIB_TARGET)

distclean: clean
	-@rm -f Makefile

depend:
	@echo depending in $(LOCATION)
	@if [ x$(DEPEND_DONE) = x ]; then \
	    echo DEPEND_DONE = 1 >> Makefile; \
	    $(DEPEND_PROG) $(INCLUDE) $(CCFILES) $(FILTER) >> Makefile; \
	fi

install:

uninstall:

#######################################################################
