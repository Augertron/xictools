timegen: timegen.cc
	$(CXX) -o timegen timegen.cc

clean distclean:
	rm -f datestr timegen

depend:
	@echo depending in util
	@if [ x$(DEPEND_DONE) = x ]; then \
	    echo DEPEND_DONE = 1 >> Makefile; \
	    $(DEPEND_PROG) timegen.cc $(FILTER) >> Makefile; \
	fi

