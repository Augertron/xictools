DATE = @DATE@
MALLOC = @MALLOC@

LPATH = .

SUBDIRS = \
    ginterf \
    miscutil \
    util

dummy: 
	@for a in $(SUBDIRS); do \
	    if [ -d $$a ]; then \
	        (cd $$a; $(MAKE)) \
	    fi; \
	done
	@if [ z$(MALLOC) != z ]; then \
	    if [ -d malloc ]; then \
	        (cd malloc; $(MAKE)) \
	    fi; \
	fi
	
depend clean:
	@for a in $(SUBDIRS); do \
	    if [ -d $$a ]; then \
	        (cd $$a; $(MAKE) $@) \
	    fi; \
	done
	@if [ z$(MALLOC) != z ]; then \
	    if [ -d malloc ]; then \
	        (cd malloc; $(MAKE) $@) \
	    fi; \
	fi

distclean:
	@for a in $(SUBDIRS) packages; do \
	    if [ -d $$a ]; then \
	        (cd $$a; $(MAKE) $@) \
	    fi; \
	done
	@if [ z$(MALLOC) != z ]; then \
	    if [ -d malloc ]; then \
	        (cd malloc; $(MAKE) $@) \
	    fi; \
	fi
	-rm -f config.cache config.log config.status configure Makefile
	-rm -rf autom4te.cache
	-rm -f miscutil/config.h


