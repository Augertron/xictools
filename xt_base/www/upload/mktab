#! /bin/bash

# Create the table file that provides version info for remote queries.

exprog=./exported.new
progs=$($exprog progs)
oses=$($exprog oses)

tabfile=os_tab

echo "# Generated by mktab" > $tabfile
for os in $oses; do
    echo -n $os
    for a in $progs; do
        vers=$($exprog version $a)
        if [ -n "$vers" ]; then
            echo -n " $a-$vers"
        fi
    done
    echo
done >> $tabfile

